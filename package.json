    "sass": "^1.87.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.1",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "author": "The Stylist Team",
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all",
    "ie 11"
  ]
}
