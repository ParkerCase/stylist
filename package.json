{
  "name": "stylist-widget",
  "version": "1.0.0",
  "description": "The Stylist - AI-powered shopping assistant widget",
  "main": "dist/stylist-widget.js",
  "scripts": {
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "build:analyze": "webpack --mode production --analyze",
    "build:profile": "webpack --mode production --profile --json > stats.json",
    "build:no-ts-check": "TS_NODE_TRANSPILE_ONLY=true webpack --mode production",
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx,scss}",
    "verify": "node verify.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test-scraper": "cd services/social-proof && ts-node __tests__/runWhoWhatWearScraper.ts",
    "diagnose": "node diagnose.js",
    "postbuild": "node scripts/optimize-assets.js"
  },
  "dependencies": {
    "@tensorflow-models/body-pix": "^2.2.0",
    "@tensorflow/tfjs": "^3.6.0",
    "@tensorflow/tfjs-backend-webgl": "^3.6.0",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "express": "^5.1.0",
    "process": "^0.11.10",
    "puppeteer": "^24.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.21.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/cypress": "^0.1.6",
    "@types/jest": "^29.4.0",
    "@types/jquery": "^3.5.32",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "babel-loader": "^9.1.2",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.3",
    "cypress": "^14.2.1",
    "cypress-file-upload": "^5.0.8",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.8.3",
    "image-webpack-loader": "^8.1.0",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webp": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.3",
    "preload-webpack-plugin": "^3.0.0-beta.4",
